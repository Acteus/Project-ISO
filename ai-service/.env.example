# Flask AI Service Configuration

# Server Configuration
FLASK_PORT=5002
FLASK_DEBUG=false
FLASK_HOST=0.0.0.0

# Laravel Integration
LARAVEL_BASE_URL=http://localhost:8000
LARAVEL_API_KEY=your-api-key-here

# Model Configuration
MODEL_SAVE_PATH=models/
DATA_CACHE_PATH=data/cache/

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/ai_service.log

# CORS Settings
CORS_ORIGINS=http://localhost:8000,http://localhost:3000

# Performance Settings
MAX_WORKERS=4
BATCH_SIZE=32
TIMEOUT_SECONDS=30
